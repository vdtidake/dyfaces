<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
	version="2.0">
	<namespace>http://dyfaces.org</namespace>
	<tag>
		<tag-name>line</tag-name>
		<component>
			<component-type>org.dyfaces.component.graph</component-type>
			<renderer-type>org.dyfaces.component.graph.renderer</renderer-type>
		</component>
		<attribute>
			<description><![CDATA[Unique identifier of the component in a namingContainer.]]>
            </description>
			<name>id</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description><![CDATA[Boolean value to specify the rendering of the component, when set to false component will not be rendered.]]>
            </description>
			<name>rendered</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<description><![CDATA[width of the viewer frame.]]>
            </description>
			<name>width</name>
			<required>false</required>
			<type>java.lang.Integer</type>
		</attribute>
		<attribute>
			<description><![CDATA[height of the viewer frame.]]>
            </description>
			<name>height</name>
			<required>false</required>
			<type>java.lang.Integer</type>
		</attribute>
		<attribute>
			<name>legend</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>title</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>showRoller</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<name>rollPeriod</name>
			<required>false</required>
			<type>java.lang.Integer</type>
		</attribute>
		<attribute>
			<name>customBars</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<name>ylabel</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>xlabel</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
            <description><![CDATA[Series of the component]]></description>
            <name>series</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>
		<attribute>
            <description><![CDATA[Datasource of the component]]></description>
            <name>model</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>
        <attribute>
            <description><![CDATA[Datasource of the component]]></description>
            <name>value</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>
        <!-- POINT SETTINGS -->
        <attribute>
			<name>drawPoints</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
		</attribute>
        <attribute>
			<name>pointSize</name>
			<required>false</required>
			<type>java.lang.Short</type>
		</attribute>
		<attribute>
			<name>highlightCircleSize</name>
			<required>false</required>
			<type>java.lang.Short</type>
		</attribute>
		 <!-- GRID SETTINGS -->
		  <attribute>
			<name>drawGrid</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
		</attribute>
		 <attribute>
			<name>drawXAxis</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
		</attribute>
		 <attribute>
			<name>drawYAxis</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
		</attribute>
		<!-- callback -->
		<attribute>
			<name>clickCallback</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>drawCallback</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>highlightCallback</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>pointClickCallback</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>underlayCallback</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>unhighlightCallback</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>zoomCallback</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<!-- Annotations -->
		 <attribute>
			<name>showAnnotations</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
		</attribute>
		 <attribute>
			<name>annotations</name>
			<required>false</required>
			<type>java.lang.Object</type>
		</attribute>
		 <attribute>
			<name>highlightRegions</name>
			<required>false</required>
			<type>java.lang.Object</type>
		</attribute>
		<attribute>
			<name>synchronize</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>tooltip</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
		</attribute>
	</tag>
</facelet-taglib>
    